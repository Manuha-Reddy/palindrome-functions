function reverseText(text) {
  return text.split('').reverse().join('');
}

function checkIfPalindrome(text) {
  const textReversed = reverseText(text);
  if (text === textReversed) {
    return true;
  } else {
    return false;
  }
}

console.log(checkIfPalindrome("hello")); 
console.log(checkIfPalindrome("racecar")); 
